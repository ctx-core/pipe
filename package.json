{
	"name": "@ctx-core/pipe",
	"version": "6.0.25",
	"description": "ctx-core pipe",
	"keywords": [
		"ctx-core",
		"pipe"
	],
	"homepage": "https://github.com/ctx-core/pipe#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pipe/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pipe/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
