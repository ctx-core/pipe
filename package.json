{
	"name": "@ctx-core/pipe",
	"version": "6.0.29",
	"description": "ctx-core pipe",
	"keywords": [
		"ctx-core",
		"pipe"
	],
	"homepage": "https://github.com/ctx-core/pipe#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/pipe/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/pipe.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
